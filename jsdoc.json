{
  "source": {
    "include": ["./nodes"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "Plugins": [
    "plugins/markdown",
    "plugins/jsdoc-babel",
    "./node_modules/jsdoc-ignore-code/index"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  }
}